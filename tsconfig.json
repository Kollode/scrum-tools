{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./built/",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es5",
        "lib": [
            "dom",
            "es2015",
            "es2016"
        ]
    },
    "include": [
        "./src/**/**/*"
    ]
}